<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="8dp"
    tools:context="rayanalkhelaiwi.me.habittrackerappdb.MainActivity">

    <TextView
        android:id="@+id/text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="DB Insertion happens here" />

    <Button
        android:id="@+id/btn_dummy_insertion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:text="@string/btn_dummy_insertion" />

    <Button
        android:id="@+id/btn_enter_insetion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/btn_dummy_insertion"
        android:layout_alignParentEnd="true"
        android:text="@string/btn_enter_insertion" />

</RelativeLayout>
